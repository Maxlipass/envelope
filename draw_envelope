import turtle
turtle.hideturtle()
turtle.setpos(-200, 0)
turtle.clear()
turtle.fd(35)
turtle.clear()
turtle.lt(90)
turtle.fd(100)
turtle.rt(90)
turtle.fd(300)
turtle.rt(90)
turtle.fd(150)
turtle.rt(90)
turtle.fd(300)
turtle.rt(90)
turtle.fd(150)
turtle.rt(135)
turtle.fd(150)
turtle.lt(45)
turtle.bk(30)
turtle.fd(150)
turtle.bk(30)
turtle.lt(45)
turtle.fd(150)
turtle.done()
